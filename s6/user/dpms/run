#!/bin/execlineb -P

importas -S ENV_DIR
s6-envdir ${ENV_DIR}

importas -i XDG_CONFIG_HOME XDG_CONFIG_HOME
importas -i XDG_RUNTIME_DIR XDG_RUNTIME_DIR
importas -i HYPRLAND_INSTANCE_SIGNATURE HYPRLAND_INSTANCE_SIGNATURE
importas -i WAYLAND_DISPLAY WAYLAND_DISPLAY
importas -i DBUS_SESSION_BUS_ADDRESS DBUS_SESSION_BUS_ADDRESS

/usr/bin/env -i

XDG_CONFIG_HOME=${XDG_CONFIG_HOME}
XDG_RUNTIME_DIR=${XDG_RUNTIME_DIR}
HYPRLAND_INSTANCE_SIGNATURE=${HYPRLAND_INSTANCE_SIGNATURE}
WAYLAND_DISPLAY=${WAYLAND_DISPLAY}
DBUS_SESSION_BUS_ADDRESS=${DBUS_SESSION_BUS_ADDRESS}

/usr/bin/hypridle
